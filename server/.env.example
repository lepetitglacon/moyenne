# Server
PORT=3000

# REQUIRED - JWT secret for authentication (no default, server will crash if missing)
SECRET_KEY=your_jwt_secret_key

# REQUIRED - API key for Discord bot (no default, server will crash if missing)
BOT_API_KEY=your_bot_api_key

# Optional - Frontend URL for CORS
FRONTEND_URL=http://localhost:5173

# Logging (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
