{
  "name": "moyenne",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "pnpm -r --parallel run dev",
    "dev:frontend": "pnpm --filter @moyenne/frontend dev",
    "dev:server": "pnpm --filter @moyenne/server dev",
    "dev:bot": "pnpm --filter @moyenne/discord-bot dev",
    "build": "pnpm --filter @moyenne/frontend build",
    "start:server": "pnpm --filter @moyenne/server start",
    "start:bot": "pnpm --filter @moyenne/discord-bot start",
    "test": "pnpm --filter @moyenne/server test"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.15.0"
}